{
  "name": "LaTeX for GitHub",
  "description": "Write LaTeX in GitHub Issues",
  "version": "0.0.3",
  "permissions": [
    "activeTab",
    "https://github.com/"
  ],
  "background": {
    "scripts": ["katex/katex.min.js", "background.js"],
    "persistent": false
  },
  "web_accessible_resources": [
    "katex/fonts/*"
  ],
  "browser_action": {
    "default_title": "LaTeX for GitHub"
  },
  "content_security_policy": "font-src 'self'; script-src 'self'; object-src 'self'",
  "manifest_version": 2
}